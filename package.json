{
  "name": "microchassis",
  "version": "0.6.6",
  "description": "",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "prebuild": "rm -rf dist/*",
    "build": "tsc --declaration",
    "lint": "tslint --project tslint.json src/**/*.ts",
    "dev": "tsc -w",
    "test": "mocha",
    "test:watch": "mocha --watch --watch-extensions ts",
    "test:coverage": "nyc -r lcov -r text npm run test",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "prepublish": "npm run lint && npm run test && npm run build"
  },
  "author": "Raymond de Wit <raymond@thewhite.nl> (http://github.com/rthewhite)",
  "contributors": [
    "Andrii Mishkovskyi <mishok13@gmail.com> (http://github.com/mishok13)"
  ],
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com:schubergphilis/microchassis.git"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "exclude": [
      "test",
      "test{,-*}.js",
      "**/*.test.js",
      "**/__tests__/**",
      "**/node_modules/**",
      "src/vendor.d.ts",
      "example",
      "dist",
      "coverage"
    ],
    "all": true
  },
  "devDependencies": {
    "@types/async": "^2.0.45",
    "@types/body-parser": "^1.16.8",
    "@types/chai": "^4.0.6",
    "@types/connect-timeout": "0.0.33",
    "@types/deepmerge": "^1.3.2",
    "@types/express": "^4.0.37",
    "@types/http-status": "^0.2.30",
    "@types/minimist": "^1.2.0",
    "@types/mocha": "^2.2.43",
    "@types/node": "^8.0.47",
    "@types/protobufjs": "^5.0.31",
    "@types/reflect-metadata": "0.0.5",
    "@types/sinon": "^2.3.5",
    "@types/sinon-chai": "^2.7.29",
    "@types/uuid": "^3.4.3",
    "chai": "^4.1.2",
    "coveralls": "^2.13.3",
    "mocha": "^5.2.0",
    "mock-express-request": "^0.2.0",
    "nodemon": "^1.18.3",
    "nyc": "^11.2.1",
    "sinon": "^2.4.1",
    "sinon-chai": "^2.14.0",
    "source-map-support": "^0.4.18",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "tslint-consistent-codestyle": "^1.13.3",
    "typescript": "^3.0.1"
  },
  "dependencies": {
    "ajv": "~5.5.1",
    "async": "^2.5.0",
    "body-parser": "^1.18.2",
    "connect-timeout": "^1.9.0",
    "deepmerge": "^1.5.2",
    "express": "^4.16.2",
    "grpc": "^1.14.1",
    "http-status": "^1.0.1",
    "http-status-codes": "^1.3.0",
    "inversify": "^4.3.0",
    "minimist": "^1.2.0",
    "mysql": "^2.14.1",
    "reflect-metadata": "^0.1.10",
    "rxjs": "^5.4.3",
    "ts-mockito": "^2.2.5",
    "typeorm": "^0.1.1",
    "uuid": "^3.1.0"
  }
}
